kBAAA,KAAA,EAAA,MAAA,CAAA,KAHA;AAIA,kBAAA,GAAA,EAAA,MAAA,CAAA,GAJA;AAKA,kBAAA,KAAA,EAAA,MAAA,CAAA;AALA,iBAAA,CAJA;;AAAA;AAYA,gBAAA,MAAA,CAAA,SAAA;;AAEA,gBAAA,MAAA,CAAA,IAAA,GAAA,IAAA;AACA,gBAAA,MAAA,CAAA,OAAA,GAAA,IAAA;AACA,gBAAA,MAAA,CAAA,KAAA,GAAA,IAAA;AACA,gBAAA,MAAA,CAAA,GAAA,GAAA,IAAA;AACA,gBAAA,MAAA,CAAA,KAAA,GAAA,SAAA;AAlBA;AAAA;;AAAA;AAqBA,gBAAA,OAAA,CAAA,GAAA,CAAA,qBAAA;;AArBA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAwBA,gBAAA,OAAA,CAAA,GAAA;;AAxBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA0BA,KAzDA;AA0DA,IAAA,SA1DA,uBA0DA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAGA,EAAA,CAAA,UAAA,CAAA,SAAA,EAAA,GAAA,EAHA;;AAAA;AAGA,gBAAA,QAHA;AAIA,gBAAA,MAJA,GAIA,EAJA;AAMA,gBAAA,QAAA,CAAA,OAAA,CAAA,UAAA,GAAA,EAAA;AACA;AACA,sBAAA,UAAA,GAAA,GAAA,CAAA,IAAA,EAAA;AACA,kBAAA,UAAA,CAAA,EAAA,GAAA,GAAA,CAAA,EAAA;AACA,kBAAA,MAAA,CAAA,IAAA,CAAA,UAAA;AACA,iBALA;AAOA,gBAAA,MAAA,CAAA,MAAA,GAAA,MAAA;AAbA;AAAA;;AAAA;AAAA;AAAA;AAgBA,gBAAA,OAAA,CAAA,GAAA;;AAhBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAkBA,KA5EA;AA6EA,IAAA,OA7EA,yBA6EA;AAAA,UAAA,IAAA,QAAA,IAAA;AACA,WAAA,KAAA,GAAA,IAAA;AACA,WAAA,IAAA,GAAA,KAAA;AACA,KAhFA;AAiFA,IAAA,aAjFA,yBAiFA,KAjFA,EAiFA;AACA,aAAA,KAAA,CAAA,KAAA;AACA,KAnFA;AAoFA,IAAA,QApFA,sBAoFA;AACA,WAAA,KAAA,GAAA,KAAA,KAAA;AACA,KAtFA;AAuFA,IAAA,IAvFA,kBAuFA;AACA,WAAA,KAAA,CAAA,QAAA,CAAA,IAAA;AACA,KAzFA;AA0FA,IAAA,IA1FA,kBA0FA;AACA,WAAA,KAAA,CAAA,QAAA,CAAA,IAAA;AACA,KA5FA;AA6FA,IAAA,SA7FA,4BA6FA;AAAA;;AAAA,UAAA,WAAA,SAAA,WAAA;AAAA,UAAA,KAAA,SAAA,KAAA;;AACA,UAAA,IAAA,GAAA,SAAA,IAAA,GAAA;AACA,QAAA,MAAA,CAAA,aAAA,GAAA,KAAA;AACA,QAAA,MAAA,CAAA,eAAA,GAAA,WAAA,CAAA,MAAA;AACA,QAAA,UAAA,CAAA;AAAA,iBAAA,MAAA,CAAA,YAAA,GAAA,IAAA;AAAA,SAAA,EAAA,EAAA,CAAA;AACA,OAJA;;AAMA,UAAA,KAAA,YAAA,EAAA;AACA,aAAA,YAAA,GAAA,KAAA;AACA,QAAA,UAAA,CAAA,IAAA,EAAA,EAAA,CAAA;AACA,OAHA,MAGA;AACA,QAAA,IAAA;AACA;;AAEA,MAAA,WAAA,CAAA,eAAA;AACA,KA5GA;AA6GA,IAAA,WA7GA,8BA6GA;AAAA,UAAA,KAAA,SAAA,KAAA;AAAA,UAAA,GAAA,SAAA,GAAA;AACA;AACA,WAAA,KAAA,GAAA,KAAA;AACA,WAAA,GAAA,GAAA,GAAA;AACA,KAjHA;AAkHA,IAAA,GAlHA,eAkHA,CAlHA,EAkHA;AACA,aAAA,CAAA,GAAA,CAAA,IAAA,CAAA,GAAA,EAAA,GACA,IADA,GAEA,CAAA,IAAA,EAAA,IAAA,EAAA,IAAA,EAAA,IAAA,EAAA,IAAA,EAAA,IAAA,EAAA,IAAA,EAAA,IAAA,EAAA,IAAA,EAAA,IAAA,EAAA,CAAA,GAAA,EAAA,CAFA;AAGA;AAtHA;AAhEA,CAAA","sourcesContent":["<template>\n  <v-row class=\"fill-height\">\n    <v-col>\n      <v-sheet height=\"64\">\n        <v-toolbar flat color=\"white\">\n          <v-btn color=\"primary\" dark class=\"mr-4\" @click=\"dialog = true\">\n            Adicionar\n          </v-btn>\n          <v-btn outlined class=\"mr-4\" @click=\"setToday\">\n            Hoje\n          </v-btn>\n          <v-btn fab text small @click=\"prev\">\n            <v-icon small>mdi-chevron-left</v-icon>\n          </v-btn>\n          <v-btn fab text small @click=\"next\">\n            <v-icon small>mdi-chevron-right</v-icon>\n          </v-btn>\n          <v-toolbar-title>{{ title }}</v-toolbar-title>\n          <v-spacer></v-spacer>\n          <v-menu bottom right>\n            <template v-slot:activator=\"{ on }\">\n              <v-btn\n                outlined\n                v-on=\"on\"\n              >\n                <span>{{ typeToLabel[type] }}</span>\n                <v-icon right>mdi-menu-down</v-icon>\n              </v-btn>\n            </template>\n            <v-list>\n              <v-list-item @click=\"type = 'day'\">\n                <v-list-item-title>Dia</v-list-item-title>\n              </v-list-item>\n              <v-list-item @click=\"type = 'week'\">\n                <v-list-item-title>Semana</v-list-item-title>\n              </v-list-item>\n              <v-list-item @click=\"type = 'month'\">\n                <v-list-item-title>Mês</v-list-item-title>\n              </v-list-item>\n              <v-list-item @click=\"type = '4day'\">\n                <v-list-item-title>4 dias</v-list-item-title>\n              </v-list-item>\n            </v-list>\n          </v-menu>\n        </v-toolbar>\n      </v-sheet>\n      <v-sheet height=\"600\">\n        \n        <v-calendar\n          ref=\"calendar\"\n          v-model=\"focus\"\n          color=\"primary\"\n          :events=\"events\"\n          :event-color=\"getEventColor\"\n          :event-margin-bottom=\"3\"\n          :now=\"today\"\n          :type=\"type\"\n          @click:event=\"showEvent\"\n          @click:more=\"viewDay\"\n          @click:date=\"viewDay\"\n          @change=\"updateRange\"\n          :weekdays=\"[1, 2, 3, 4, 5, 6, 0]\"\n          locale=\"pt\"\n          :short-weekdays=\"false\"\n        ></v-calendar>\n\n        <!-- Modal Adicionar Evento -->\n        <v-dialog v-model=\"dialog\">\n          <v-card>\n            <v-container>\n              <v-form @submit.prevent=\"addEvent\">\n                <v-text-field \n                  type=\"text\" label=\"Nome do evento\" v-model=\"name\">\n                </v-text-field>\n                <v-text-field \n                  type=\"text\" label=\"Detalhes do evento\" v-model=\"details\">\n                </v-text-field>\n                <v-text-field \n                  type=\"date\" label=\"Início\" v-model=\"start\">\n                </v-text-field>\n                <v-text-field \n                  type=\"date\" label=\"Fim\" v-model=\"end\">\n                </v-text-field>\n                <v-text-field \n                  type=\"color\" label=\"Cor da marcação\" v-model=\"color\">\n                </v-text-field>\n                <v-btn type=\"submit\" color=\"primary\" class=\"mr-4\" \n                @click.stop=\"dialog = false\">Adicionar</v-btn>\n              </v-form>\n            </v-container>\n          </v-card>\n        </v-dialog>\n\n\n        <v-menu\n          v-model=\"selectedOpen\"\n          :close-on-content-click=\"false\"\n          :activator=\"selectedElement\"\n          offset-x\n        >\n          <v-card\n            color=\"grey lighten-4\"\n            min-width=\"350px\"\n            flat\n          >\n            <v-toolbar\n              :color=\"selectedEvent.color\"\n              dark\n            >\n              <v-btn icon @click=\"deleteEvent(selectedEvent)\">\n                <v-icon>mdi-delete</v-icon>\n              </v-btn>\n              <v-toolbar-title v-html=\"selectedEvent.name\"></v-toolbar-title>\n              <v-spacer></v-spacer>\n            </v-toolbar>\n\n\n            <v-card-text>\n             \n              <v-form v-if=\"currentlyEditing !== selectedEvent.id\">\n                {{selectedEvent.name}} - {{selectedEvent.details}}\n              </v-form>\n\n              <v-form v-else>\n\n                <v-text-field \n                  type=\"text\" v-model=\"selectedEvent.name\"\n                  label=\"Editar evento\">\n                </v-text-field>\n\n                <textarea-autosize\n                  v-model=\"selectedEvent.details\"\n                  type=\"text\"\n                  style=\"width: 100%\"\n                  :min-height=\"100\"\n                ></textarea-autosize>\n\n              </v-form>\n\n            </v-card-text>\n\n            \n            <v-card-actions>\n              <v-btn\n                text\n                color=\"secondary\"\n                @click=\"selectedOpen = false\"\n              >\n                Cancel\n              </v-btn>\n              <v-btn text v-if=\"currentlyEditing !== selectedEvent.id\"\n              @click.prevent=\"editEvent(selectedEvent.id)\">Editar</v-btn>\n\n              <v-btn text v-else  @click.prevent=\"updateEvent(selectedEvent)\">Atualizar</v-btn>\n            </v-card-actions>\n          </v-card>\n        </v-menu>\n      </v-sheet>\n    </v-col>\n  </v-row>\n</template>\n\n<script>\n  import {db} from '../main'\n\n  export default {\n    data: () => ({\n      today: new Date().toISOString().substr(0,10),\n      focus: new Date().toISOString().substr(0,10),\n      type: 'month',\n      typeToLabel: {\n        month: 'Mes',\n        week: 'Week',\n        day: 'Day',\n        '4day': '4 Days',\n      },\n      start: null,\n      end: null,\n      selectedEvent: {},\n      selectedElement: null,\n      selectedOpen: false,\n      events: [],\n      name: null,\n      details: null,\n      color: '#1976D2',\n      dialog: false,\n      currentlyEditing: null\n    }),\n    computed: {\n      title () {\n        const { start, end } = this\n        if (!start || !end) {\n          return ''\n        }\n\n        const startMonth = this.monthFormatter(start)\n        const endMonth = this.monthFormatter(end)\n        const suffixMonth = startMonth === endMonth ? '' : endMonth\n\n        const startYear = start.year\n        const endYear = end.year\n        const suffixYear = startYear === endYear ? '' : endYear\n\n        const startDay = start.day + this.nth(start.day)\n        const endDay = end.day + this.nth(end.day)\n\n        switch (this.type) {\n          case 'month':\n            return `${startMonth} ${startYear}`\n          case 'week':\n          case '4day':\n            return `${startMonth} ${startDay} ${startYear} - ${suffixMonth} ${endDay} ${suffixYear}`\n          case 'day':\n            return `${startMonth} ${startDay} ${startYear}`\n        }\n        return ''\n      },\n      monthFormatter () {\n        return this.$refs.calendar.getFormatter({\n          timeZone: 'UTC', month: 'long',\n        })\n      },\n    },\n    mounted () {\n      this.$refs.calendar.checkChange();\n    },\n    created(){\n      this.getEvents();\n    },\n    methods: {\n      async updateEvent(ev){\n        try {\n\n          await db.collection('eventos').doc(ev.id).update({\n            name: ev.name,\n            details: ev.details\n          })\n\n          this.selectedOpen = false;\n          this.currentlyEditing = null;\n\n          \n        } catch (error) {\n          console.log(error);\n        }\n      },\n      editEvent(id){\n        this.currentlyEditing = id\n      },\n      async deleteEvent(ev){\n        try {\n\n          await db.collection('eventos').doc(ev.id).delete();\n          this.selectedOpen = false;\n          this.getEvents();\n          \n        } catch (error) {\n          console.log(error);\n        }\n      },\n      async addEvent(){\n        try {\n          if(this.name && this.start && this.end){\n\n            await db.collection('eventos').add({\n              name: this.name,\n              details: this.details,\n              start: this.start,\n              end: this.end,\n              color: this.color\n            })\n\n            this.getEvents();\n\n            this.name = null;\n            this.details = null;\n            this.start = null;\n            this.end = null;\n            this.color = '#1976D2';\n\n          }else{\n            console.log('Campos obrigatórios');\n          }\n        } catch (error) {\n          console.log(error);\n        }\n      },\n      async getEvents(){\n        try {\n\n          const snapshot = await db.collection('eventos').get();\n          const events = [];\n\n          snapshot.forEach(doc => {\n            // console.log(doc.id);\n            let eventoData = doc.data();\n            eventoData.id = doc.id;\n            events.push(eventoData);\n          })\n\n          this.events = events;\n          \n        } catch (error) {\n          console.log(error);\n        }\n      },\n      viewDay ({ date }) {\n        this.focus = date\n        this.type = 'day'\n      },\n      getEventColor (event) {\n        return event.color\n      },\n      setToday () {\n        this.focus = this.today\n      },\n      prev () {\n        this.$refs.calendar.prev()\n      },\n      next () {\n        this.$refs.calendar.next()\n      },\n      showEvent ({ nativeEvent, event }) {\n        const open = () => {\n          this.selectedEvent = event\n          this.selectedElement = nativeEvent.target\n          setTimeout(() => this.selectedOpen = true, 10)\n        }\n\n        if (this.selectedOpen) {\n          this.selectedOpen = false\n          setTimeout(open, 10)\n        } else {\n          open()\n        }\n\n        nativeEvent.stopPropagation()\n      },\n      updateRange ({ start, end }) {\n        // You could load events from an outside source (like database) now that we have the start and end dates on the calendar\n        this.start = start\n        this.end = end\n      },\n      nth (d) {\n        return d > 3 && d < 21\n          ? 'th'\n          : ['th', 'st', 'nd', 'rd', 'th', 'th', 'th', 'th', 'th', 'th'][d % 10]\n      },\n    },\n  }\n</script>\n\n\nfs.inotify.max_user_watches=524288"],"sourceRoot":"src/components"}]}