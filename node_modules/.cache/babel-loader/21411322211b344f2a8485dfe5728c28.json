AACA,gBAAA,MAAA,CAAA,GAAA,GAAA,EAAA;AACA,gBAAA,MAAA,CAAA,KAAA,GAAA,SAAA;AAfA;AAAA;;AAAA;AAiBA,gBAAA,KAAA,CAAA,qBAAA,CAAA;;AAjBA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAoBA,gBAAA,OAAA,CAAA,GAAA;;AApBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAsBA,KAnCA;AAsCA,IAAA,SAtCA,uBAsCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAGA,EAAA,CAAA,UAAA,CAAA,WAAA,EAAA,GAAA,EAHA;;AAAA;AAGA,gBAAA,QAHA;AAIA,gBAAA,MAJA,GAIA,EAJA;AAMA,gBAAA,QAAA,CAAA,OAAA,CAAA,UAAA,GAAA,EAAA;AACA;AACA,sBAAA,UAAA,GAAA,GAAA,CAAA,IAAA,EAAA;AACA,kBAAA,UAAA,CAAA,EAAA,GAAA,GAAA,CAAA,EAAA;AACA,kBAAA,MAAA,CAAA,IAAA,CAAA,UAAA;AACA,iBALA;AAOA,gBAAA,MAAA,CAAA,MAAA,GAAA,MAAA;AAbA;AAAA;;AAAA;AAAA;AAAA;AAgBA,gBAAA,OAAA,CAAA,GAAA;;AAhBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAmBA,KAzDA;AA0DA,IAAA,OA1DA,yBA0DA;AAAA,UAAA,IAAA,QAAA,IAAA;AACA,WAAA,KAAA,GAAA,IAAA;AACA,WAAA,IAAA,GAAA,KAAA;AACA,KA7DA;AA8DA,IAAA,aA9DA,yBA8DA,KA9DA,EA8DA;AACA,aAAA,KAAA,CAAA,KAAA;AACA,KAhEA;AAiEA,IAAA,QAjEA,sBAiEA;AACA,WAAA,KAAA,GAAA,KAAA,KAAA;AACA,KAnEA;AAoEA,IAAA,IApEA,kBAoEA;AACA,WAAA,KAAA,CAAA,QAAA,CAAA,IAAA;AACA,KAtEA;AAuEA,IAAA,IAvEA,kBAuEA;AACA,WAAA,KAAA,CAAA,QAAA,CAAA,IAAA;AACA,KAzEA;AA0EA,IAAA,SA1EA,4BA0EA;AAAA;;AAAA,UAAA,WAAA,SAAA,WAAA;AAAA,UAAA,KAAA,SAAA,KAAA;;AACA,UAAA,IAAA,GAAA,SAAA,IAAA,GAAA;AACA,QAAA,MAAA,CAAA,aAAA,GAAA,KAAA;AACA,QAAA,MAAA,CAAA,eAAA,GAAA,WAAA,CAAA,MAAA;AACA,QAAA,UAAA,CAAA;AAAA,iBAAA,MAAA,CAAA,YAAA,GAAA,IAAA;AAAA,SAAA,EAAA,EAAA,CAAA;AACA,OAJA;;AAMA,UAAA,KAAA,YAAA,EAAA;AACA,aAAA,YAAA,GAAA,KAAA;AACA,QAAA,UAAA,CAAA,IAAA,EAAA,EAAA,CAAA;AACA,OAHA,MAGA;AACA,QAAA,IAAA;AACA;;AAEA,MAAA,WAAA,CAAA,eAAA;AACA,KAzFA;AA0FA,IAAA,WA1FA,8BA0FA;AAAA,UAAA,KAAA,SAAA,KAAA;AAAA,UAAA,GAAA,SAAA,GAAA;AACA;AACA,WAAA,KAAA,GAAA,KAAA;AACA,WAAA,GAAA,GAAA,GAAA;AACA,KA9FA;AA+FA,IAAA,GA/FA,eA+FA,CA/FA,EA+FA;AACA,aAAA,CAAA,GAAA,CAAA,IAAA,CAAA,GAAA,EAAA,GACA,IADA,GAEA,CAAA,IAAA,EAAA,IAAA,EAAA,IAAA,EAAA,IAAA,EAAA,IAAA,EAAA,IAAA,EAAA,IAAA,EAAA,IAAA,EAAA,IAAA,EAAA,IAAA,EAAA,CAAA,GAAA,EAAA,CAFA;AAGA;AAnGA;AA/DA,CAAA","sourcesContent":["<template>\n  <v-row class=\"fill-height\">\n    <v-col>\n      <v-sheet height=\"64\">\n        <v-toolbar flat color=\"white\">\n\n          <!-- Botão Adicionar Evento -->\n          <v-btn color=\"primary\" dark class=\"mr-4\" @click=\"dialog = true\">Adicionar</v-btn>\n          <v-btn outlined class=\"mr-4\" @click=\"setToday\">\n            Hoje\n          </v-btn>\n          <v-btn fab text small @click=\"prev\">\n            <v-icon small>mdi-chevron-left</v-icon>\n          </v-btn>\n          <v-btn fab text small @click=\"next\">\n            <v-icon small>mdi-chevron-right</v-icon>\n          </v-btn>\n          <v-toolbar-title>{{ title }}</v-toolbar-title>\n          <v-spacer></v-spacer>\n          <v-menu bottom right>\n            <template v-slot:activator=\"{ on }\">\n              <v-btn\n                outlined\n                v-on=\"on\"\n              >\n                <span>{{ typeToLabel[type] }}</span>\n                <v-icon right>mdi-menu-down</v-icon>\n              </v-btn>\n            </template>\n            <v-list>\n              <v-list-item @click=\"type = 'day'\">\n                <v-list-item-title>Dia</v-list-item-title>\n              </v-list-item>\n              <v-list-item @click=\"type = 'week'\">\n                <v-list-item-title>Semana</v-list-item-title>\n              </v-list-item>\n              <v-list-item @click=\"type = 'month'\">\n                <v-list-item-title>Mês</v-list-item-title>\n              </v-list-item>\n              <v-list-item @click=\"type = '4day'\">\n                <v-list-item-title>4 days</v-list-item-title>\n              </v-list-item>\n            </v-list>\n          </v-menu>\n        </v-toolbar>\n      </v-sheet>\n      <v-sheet height=\"600\">\n        <v-calendar\n          ref=\"calendar\"\n          v-model=\"focus\"\n          color=\"primary\"\n          :events=\"events\"\n          :event-color=\"getEventColor\"\n          :event-margin-bottom=\"3\"\n          :now=\"today\"\n          :type=\"type\"\n          @click:event=\"showEvent\"\n          @click:more=\"viewDay\"\n          @click:date=\"viewDay\"\n          @change=\"updateRange\"\n          :weekdays=\"[1, 2, 3, 4, 5, 6, 0]\"\n          locale=\"pt\"\n          :short-weekdays=\"false\"\n        ></v-calendar>\n\n        <!-- Adicionar Modal Adicionar Evento -->\n       <v-dialog v-model=\"dialog\" max-width=\"500\">\n  <v-card>\n    <v-container>\n      <v-form @submit.prevent=\"addEvent\">\n        <v-text-field v-model=\"name\" type=\"text\" label=\"Nome\"></v-text-field>\n        <v-text-field v-model=\"details\" type=\"text\" label=\"Detalhes\"></v-text-field>\n        <v-text-field v-model=\"start\" type=\"date\" label=\"Início\"></v-text-field>\n        <v-text-field v-model=\"end\" type=\"date\" label=\"Fim\"></v-text-field>\n        <v-text-field v-model=\"color\" type=\"color\" label=\"cor\"></v-text-field>\n        <v-btn type=\"submit\" color=\"primay\" class=\"mr-4\" @click.stop=\"dialog = false\">Adicionar</v-btn>\n      </v-form>\n    </v-container>\n  </v-card>\n</v-dialog> \n        <v-menu\n          v-model=\"selectedOpen\"\n          :close-on-content-click=\"false\"\n          :activator=\"selectedElement\"\n          offset-x\n        >\n          <v-card\n            color=\"grey lighten-4\"\n            min-width=\"350px\"\n            flat\n          >\n            <!-- Adicionar Funcionalidade Editar e Excluir -->\n            <v-toolbar\n              :color=\"selectedEvent.color\"\n              dark\n            >\n              <v-btn icon @click=\"deleteEvent()\">\n                <v-icon>mdi-delete</v-icon>\n              </v-btn>\n              <v-toolbar-title v-html=\"selectedEvent.name\"></v-toolbar-title>\n              <v-spacer></v-spacer>\n              <v-btn icon>\n                <v-icon>mdi-heart</v-icon>\n              </v-btn>\n              <v-btn icon>\n                <v-icon>mdi-dots-vertical</v-icon>\n              </v-btn>\n            </v-toolbar>\n            <v-card-text>\n              <span v-html=\"selectedEvent.details\"></span>\n            </v-card-text>\n            <v-card-actions>\n              <v-btn\n                text\n                color=\"secondary\"\n                @click=\"selectedOpen = false\"\n              >\n                Cancel\n              </v-btn>\n            </v-card-actions>\n          </v-card>\n        </v-menu>\n      </v-sheet>\n    </v-col>\n  </v-row>\n</template>\n\n<script>\n  import {db} from '../main'\n  export default {\n    data: () => ({\n      today: new Date().toISOString().substr(0, 10),\n      focus: new Date().toISOString().substr(0, 10),\n      type: 'month',\n      typeToLabel: {\n        month: 'Mês',\n        week: 'Semana',\n        day: 'Dia',\n        '4day': '4 Dias',\n      },\n      start: null,\n      end: null,\n      selectedEvent: {},\n      selectedElement: null,\n      selectedOpen: false,\n      events: [],\n      // Adicionais\n      name: null,\n      details: null,\n      color: '#e76f51',\n      dialog: false,\n      currentlyEditing: null\n    }),\n    computed: {\n      title () {\n        const { start, end } = this\n        if (!start || !end) {\n          return ''\n        }\n\n        const startMonth = this.monthFormatter(start)\n        const endMonth = this.monthFormatter(end)\n        const suffixMonth = startMonth === endMonth ? '' : endMonth\n\n        const startYear = start.year\n        const endYear = end.year\n        const suffixYear = startYear === endYear ? '' : endYear\n\n        const startDay = start.day + this.nth(start.day)\n        const endDay = end.day + this.nth(end.day)\n\n        switch (this.type) {\n          case 'month':\n            return `${startMonth} ${startYear}`\n          case 'week':\n          case '4day':\n            return `${startMonth} ${startDay} ${startYear} - ${suffixMonth} ${endDay} ${suffixYear}`\n          case 'day':\n            return `${startMonth} ${startDay} ${startYear}`\n        }\n        return ''\n      },\n      monthFormatter () {\n        return this.$refs.calendar.getFormatter({\n          timeZone: 'UTC', month: 'long',\n        })\n      },\n    },\n    mounted () {\n      this.$refs.calendar.checkChange();\n      this.getEvents();\n    },\n    methods: {\n      async deleteEvent(ev){\n        try {\n\n          await db.collection('calEvent').doc(ev.id).delete();\n          this.selectedOpen = false;\n          this.getEvents();\n\n        } catch(error){\n          console.log(error);\n\n        }\n      },\n      async addEvent(){\n  try {\n    if(this.name && this.start && this.end){\n      await db.collection('calEvent').add({\n        name: this.name,\n        details: this.details,\n        start: this.start,\n        end: this.end,\n        color: this.color\n      })\n      this.getEvents();\n      this.name = '';\n      this.details = '';\n      this.start = '';\n      this.end = '';\n      this.color = '#1976D2';\n    }else{\n      alert('Campos obligatorios')\n    }\n  } catch (error) {\n    console.log(error);\n  }\n},\n\n      \n      async getEvents(){\n        try{\n\n          const snapshot = await db.collection('CalEvents').get();\n          const events = [];\n\n          snapshot.forEach(doc => {\n            //console.log(doc.id());\n            let eventoData = doc.data();\n            eventoData.id = doc.id;\n            events.push(eventoData);\n          })\n\n          this.events = events;\n\n        } catch (error){\n          console.log(error);\n        }\n\n      },\n      viewDay ({ date }) {\n        this.focus = date\n        this.type = 'day'\n      },\n      getEventColor (event) {\n        return event.color\n      },\n      setToday () {\n        this.focus = this.today\n      },\n      prev () {\n        this.$refs.calendar.prev()\n      },\n      next () {\n        this.$refs.calendar.next()\n      },\n      showEvent ({ nativeEvent, event }) {\n        const open = () => {\n          this.selectedEvent = event\n          this.selectedElement = nativeEvent.target\n          setTimeout(() => this.selectedOpen = true, 10)\n        }\n\n        if (this.selectedOpen) {\n          this.selectedOpen = false\n          setTimeout(open, 10)\n        } else {\n          open()\n        }\n\n        nativeEvent.stopPropagation()\n      },\n      updateRange ({ start, end }) {\n        // You could load events from an outside source (like database) now that we have the start and end dates on the calendar\n        this.start = start\n        this.end = end\n      },\n      nth (d) {\n        return d > 3 && d < 21\n          ? 'th'\n          : ['th', 'st', 'nd', 'rd', 'th', 'th', 'th', 'th', 'th', 'th'][d % 10]\n      },\n    },\n  }\n</script>\n\nfs.inotify.max_user_watches=524288"],"sourceRoot":"src/components"}]}